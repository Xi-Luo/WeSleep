@import "../../colorui/main.wxss";
@import "../../colorui/icon.wxss";
.head {
  font:bold;
  display: flex;
  justify-content: center;
  align-content: center;
  font-size: 40rpx;
}
.title{
  font-size: 20rpx;
  font: 2em sans-serif;
  text-align: center;
  padding: 10rpx;
  width: 100%;
}
.sameSize{
  margin: 0rpx 0rpx 0rpx 150rpx;
  width: 500rpx;
  height: 600rpx;
}
.thudernail{
  width: 100rpx;
  height: 150rpx;
}
.malehair{
  margin: 25rpx 0rpx 0rpx 150rpx;
  width: 500rpx;
  height: 500rpx;
  position: absolute;
  z-index: 2;
  left: 0;
}

.eye{
  margin: 185rpx 0rpx 0rpx 280rpx;
  /* 上右下左 */
  width: 250rpx;
  height: 250rpx;
  position: absolute;
  z-index: 3;
  left: 0;
}

.glass{
  margin: 165rpx 0rpx 0rpx 255rpx;
  width: 290rpx;
  height: 290rpx;
  position: absolute;
  z-index: 4;
  left: 0;
}
.cloth{
  margin: 235rpx 0rpx 0rpx 250rpx;
  width: 300rpx;
  height: 300rpx;
  position: absolute;
  z-index: 1;
  left: 0;
}

.mouth{
  width: 120rpx;
  height: 120rpx;
  margin: 330rpx 0px 0px 340rpx;
  position: absolute;
  z-index: 3;
  left: 0;
}

page{
  background: #fff;
}
.top-position{
  border-radius: 200rpx;
  position: relative;
  top: -10rpx;
  right: -10rpx;
  padding: 0rpx 10rpx;
  height: 28rpx;
  color: var(--white);
  margin-top: -210rpx;
}

.finishBtn{
  position: fixed;
  width: 100rpx;
  height: 100rpx;
  z-index: 2;
  right: 4%;
  top: 40%;
}

.bg-green {
	background-color: rgb(248, 219, 100);
	color: #ffffff;
}

.nav {
	white-space: nowrap;
}

.text-center {
	text-align: center;
}

.text-red,
.line-red,
.lines-red {
	color: #e54d42;
}

.text-white,
.line-white,
.lines-white {
	color: #ffffff;
}

.cuIcon-clothesfill:before {
  content: "\e716";
}

.cu-list.grid.col-3>.cu-item:nth-child(3n):after,
.cu-list.grid.col-4>.cu-item:nth-child(4n):after,
.cu-list.grid.col-5>.cu-item:nth-child(5n):after {
	border-right-width: 0
}

