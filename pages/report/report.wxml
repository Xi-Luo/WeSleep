<view class="portrait">
  <image src="/../img/portrait.png" class="profile" animation="{{animPlus}}" bindtap="plus"></image>
</view>

<view class="page-head-line"></view>
<view class="message">
  <view class="message_sleep">\n平均起床时间：{{wake}}</view>
  <view class="message_sleep">\n平均入睡时间：{{asleep}}</view>
  <view class="message_sleep">\n平均睡眠时长：{{time}}小时\n</view>
</view>


<view class="splite-line-dotted"></view>
<text>\n</text>

<view class='echart_wrap'>
  <ec-canvas id="mychart" canvas-id="mychart-line" ec="{{ ec }}"></ec-canvas>
</view>
<view class="splite-line-dotted"></view>
<text>\n</text>


<view class="card">
  <view class="report_head">一周健康建议</view>
  <view class="tipCard">
    <text>{{judge}}\n{{damage}}</text>
  </view>
  <view class="tipCard">
    <text>因此，小故给你的建议是：{{advice}} </text>
  </view>



</view>